package com.company;

import java.util.Scanner;

public class printsum {
    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
        System.out.println("enter two numbers");
        int a = sc.nextInt();
        int b =sc.nextInt();
        int sum = a+b;
        System.out.println("sum of two given number is :"+ sum);

    }
}
