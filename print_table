package com.company;

import java.util.Scanner;

public class tableprint {
    public static void main(String[] args) {
        Scanner sd = new Scanner(System.in);
        System.out.println("enter number for printing table");
        int num= sd.nextInt();
        for(int i =0;i<11;i++)
            System.out.println(num+"X"+i+"="+num*i);
    }
}
