package com.company;
//make a function to multiply 2 numbers and return the product.
import java.util.Scanner;

public class func_product {
    public static int cal_product(int a , int b){
        int product = a*b;
        return product;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b =sc.nextInt();
        int product =cal_product(a,b);
        System.out.println(product);
        //" or write like this System.out.println(product of two number is" + cal_product(a,b);
    }
}
